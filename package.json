{
  "name": "cloneproject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev": "react-native run-android --mode=developmentDebug --appId com.cloneproject.development",
    "android:prod": "react-native run-android --mode=productionDebug --appId com.cloneproject",
    "android:build:dev": "cd android && ./gradlew assembleDevelopmentRelease",
    "android:bundle:dev": "cd android && ./gradlew bundleDevelopmentRelease",
    "android:build:prod": "cd android && ./gradlew assembleProductionRelease",
    "android:bundle:prod": "cd android && ./gradlew bundleProductionRelease",
    "ios:dev": "react-native run-ios --scheme \"cloneproject development\"",
    "ios:prod": "react-native run-ios --scheme \"cloneproject production\"",
    "clean": "npm run android:clean && npm run ios:clean && npm run start:clean",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.0",
    "@legendapp/list": "^2.0.16",
    "@notifee/react-native": "^9.1.8",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-documents/picker": "^11.0.3",
    "@react-native-firebase/app": "^23.5.0",
    "@react-native-firebase/auth": "^23.5.0",
    "@react-native-firebase/database": "^23.5.0",
    "@react-native-firebase/storage": "^23.5.0",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-native-picker/picker": "^2.11.4",
    "@react-native-vector-icons/ionicons": "^12.3.0",
    "@react-native/new-app-screen": "0.82.1",
    "@react-navigation/native": "^7.1.20",
    "@react-navigation/native-stack": "^7.6.3",
    "@tanstack/react-query": "^5.90.10",
    "axios": "^1.13.2",
    "react": "19.1.1",
    "react-hook-form": "^7.66.1",
    "react-native": "0.82.1",
    "react-native-config": "^1.6.0",
    "react-native-date-picker": "5.0.12",
    "react-native-image-picker": "^8.2.1",
    "react-native-mmkv": "^4.0.0",
    "react-native-nitro-modules": "^0.31.6",
    "react-native-reanimated": "^4.1.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-worklets": "^0.7.1",
    "zod": "3.23.8"
  },
